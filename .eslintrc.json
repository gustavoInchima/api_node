{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "plugin:prettier/recommended", "eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "never"],
    "@typescript-eslint/semi": "off",
    "no-unexpected-multiline": "error"
  }
}
